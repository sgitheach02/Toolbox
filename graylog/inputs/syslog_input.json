{
    "title": "Syslog UDP",
    "type": "org.graylog2.inputs.syslog.udp.SyslogUDPInput",
    "configuration": {
        "bind_address": "0.0.0.0",
        "port": 1514,
        "recv_buffer_size": 262144,
        "allow_override_date": true,
        "force_rdns": false,
        "store_full_message": true
    },
    "global": true
}
